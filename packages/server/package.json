{
  "name": "@generator0/server",
  "version": "1.0.0",
  "description": "Meta Context Prompt Server for Cursor image-to-code generation",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/index.js",
    "dev:cli": "nodemon src/index.js --stdio",
    "mcp inspector": "npx @modelcontextprotocol/inspector && pnpm start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "husky"
  },
  "keywords": [
    "mcp",
    "cursor",
    "prompt",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^0.8.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "multer": "1.4.5-lts.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@google/generative-ai": "^0.24.0",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "zod": "^3.24.2"
  },
  "volta": {
    "node": "22.14.0"
  }
}
